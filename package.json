{
  "name": "gatsby-remark-obsidian-syntax",
  "version": "0.0.7",
  "description": "obsidian syntax support in gatsby",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry-run": "standard-version --dry-run",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "obsidian"
  ],
  "author": "vespucci95 <donglonk@naver.com>",
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.15.18",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "gatsby": "^5.14.3",
    "jest": "^29.7.0",
    "remark": "^15.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.3.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "unist-util-visit": "^5.0.0"
  }
